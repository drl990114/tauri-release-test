{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Releasetest",
  "version": "0.3.0",
  "identifier": "com.drl990114.releasetest",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1430",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "http://api.upgrade.toolsetlink.com/v1/tauri/upgrade?tauriKey=bj4K5BaVltyzWhyYgG0Skg&versionName={{current_version}}&appointVersionName=&devModelKey=&devKey=&target={{target}}&arch={{arch}}"
      ],
      "active": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IENCQUNGM0E0ODRCMzIwMDcKUldRSElMT0VwUE9zeTVWb1RHVTVvdnhuS2RXenY3THNrWEhWeUQ4eEdzdXZnZHI1YkYrZ1M0WnkK",
      "windows": {
        "installMode": "passive"
      },
      "dangerousInsecureTransportProtocol": true
    }
  },
   "app": {
    "windows": [
      {
        "title": "main",
        "width": 800,
        "height": 600,
        "url": "index.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
